package com.example.android.challengemestructure;

import java.util.ArrayList;
import java.util.List;

import com.example.android.navigationdrawerexample.R;


public class ListCourses {
	List<Course> listeCourses = new ArrayList<Course>();
	
	public ListCourses(){
		listeCourses.add(new Course("defi", "12/12/2014", "17:30", 7, "00:31:00", R.drawable.earth));
		listeCourses.add(new Course("defi", "12/12/2014", "17:30", 8, "00:31:00", R.drawable.earth));
		listeCourses.add(new Course("defi", "12/12/2014", "17:30", 9, "00:31:00", R.drawable.earth));
		listeCourses.add(new Course("defi", "12/12/2014", "17:30", 10, "00:31:00", R.drawable.earth));
		listeCourses.add(new Course("defi", "12/12/2014", "17:30", 11, "00:31:00", R.drawable.earth));
	}
	
	public List<Course> getListeCourses(){
		return listeCourses;
	}
}